@section MyScript{
    <script>
        $(document).ready(function () {
            $.validator.addMethod("checkType", function (value, element,) {
                if (value === "REGISTER" || value === "DETAIL"
                    || value === "ORTHER") {
                    return true;
                }
                return false;
            }, "value is invalid !");

            $.validator.addMethod("FormatPhoneNumber", function (value, element,) {
                var vnf_regex = /((09|03|07|08|05)+([0-9]{8})\b)/g;
                if (vnf_regex.test(value)) {
                    return true;
                }
                return false;
            }, "SDT phai cua Viet Name");

            $('form[name="feedback"]').validate({
                rules: {
                    txtEmailFb: {
                        required: true,
                        email: true,
                    },
                    txtName: {
                        required: true,
                    },
                    txtContent: {
                        required: true,
                    },
                    txtPhone: {
                        minlength: 10,
                        FormatPhoneNumber: true
                    },
                    feedbackType: {
                        checkType : true
                    }
                },
                messages: {
                    txtEmailFb: {
                        required: "please enter your email !",
                        email: "Email Invalid !",
                    },
                    txtName: {
                        required: "please enter your name !",
                    },
                    txtContent: {
                        required: "please enter your content !",
                    },
                    txtPhone: {
                        minlength: "phone number must have 10 numbers ",
                        FormatPhoneNumber: "phone number must be Vietnam area code"
                    },
                    feedbackType: {
                        checkType: "type is invalid !"
                    }
                }
            });

            $(document).on('click', '.sendFeedback', function () {
                var validateForm = $('form[name="feedback"]').valid();
                if (validateForm) {
                    var email = $('#txtEmailFb').val();
                    var name = $('#txtName').val();
                    var phone = $('#txtPhone').val();
                    var type = $('#feedbackType').val();
                    var content = $('#txtContent').val();

                    $.ajax({
                        url: "/home/Feedback",
                        method: "POST",
                        data: { email: email, name: name, phone: phone, type: type, content: content },
                        success: function (data) {
                            if (data.code == 200) {
                                email = $('#txtEmail').val("");
                                name = $('#txtName').val("");
                                phone = $('#txtPhone').val("");
                                type = $('#feedbackType').val("");
                                content = $('#txtContent').val("");
                                $('#myModal').modal('toggle');
                                $('#alert').modal();

                                console.log(data)
                            }
                        }
                    });
                }

            })
        });
    </script>
}

